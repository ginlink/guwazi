{
  "name": "@ginlink/translate",
  "description": "A translate app",
  "version": "0.1.0",
  "license": "MIT",
  "author": "ginlink",
  "main": "dist/main.js",
  "scripts": {
    "predev": "rimraf dist",
    "dev": "run-p dev:*",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html && wait-on ./dist/main.js && cross-env NODE_ENV=\"development\" electron .",
    "build": "cross-env NODE_ENV=\"production\" webpack --progress",
    "package": "ts-node ./builder.ts",
    "lint": "eslint ./src/**/*.{js,jsx,ts,tsx}",
    "fix": "eslint --fix ./src/**/*.{js,jsx,ts,tsx}"
  },
  "dependencies": {
    "@iconify/react": "^3.2.2",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rebass": "^4.0.7",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@ginlink/eslint-config": "^0.1.2",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/rebass": "^4.0.10",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.15",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^19.0.10",
    "electron-builder": "^23.1.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-search-devtools": "^1.8.5",
    "eslint": "^8.15.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
